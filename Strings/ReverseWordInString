class Solution {
    String reverseWords(String s) {
    s=s+" ";
    String w="",ans="";
    for(int i=0;i<s.length();i++){
        char ch = s.charAt(i);
        if(ch==' '){
         ans = w +ans ; 
           w="";
        }
        if(ch=='.'){
           ans = "."+w+ans ; 
             w="";
        }
        else{
            w=w+ch;
        }
        
    }
    return ans;
    }
}
